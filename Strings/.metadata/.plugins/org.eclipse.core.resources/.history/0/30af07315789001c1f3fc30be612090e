#include <iostream>
#include<string.h>
#include<sstream.h>
using namespace std;
string remove_spaces(string str)
{
	stringstream ss(str);
	string temp="";
	str="";
	while(ss>>temp)
	{
		str+=temp;
	}

	return temp;

}
int main() {

	string word;



	int number;

	cin>>word;




	return 0;
}
